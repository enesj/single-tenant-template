{
  "name": "dev-example-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf resources/app/public/js && rm -rf .shadow-cljs",
    "postcss": "NODE_ENV=production postcss src/app/frontend/ui/style.css -o resources/public/assets/css/style.css",
    "postcss:watch": "NODE_ENV=development TAILWIND_MODE=watch postcss src/app/frontend/ui/style.css -o resources/public/assets/css/style.css -w --verbose",
    "postcss:watch:debounced": "node scripts/postcss-watch.mjs",
    "develop": "npm run postcss:watch:debounced",
    "develop:no-debounce": "npm run postcss:watch",
    "build": "npm run postcss",
    "test:cljs": "npx shadow-cljs compile test-node && node target/test-node.cjs",
    "test:cljs:ci": "npx shadow-cljs compile test-node && node target/test-node.cjs",
    "test:cljs:watch": "npx shadow-cljs watch test",
    "test:cljs:karma:compile": "shadow-cljs compile karma-test",
    "test:cljs:karma:run": "karma start karma.conf.cjs",
    "test:cljs:karma": "npm-run-all --serial test:cljs:karma:compile test:cljs:karma:run"
  },
  "dependencies": {
    "@js-joda/core": "^5.6.3",
    "@js-joda/locale_en-us": "^4.14.0",
    "@js-joda/timezone": "2.21.1",
    "create-react-class": "~15.7.0",
    "cssnano": "^7.0.6",
    "daisyui": "^5.0.4",
    "echarts-for-react": "^3.0.2",
    "jsdom": "^25.0.1",
    "jsdom-global": "^3.0.2",
    "katex": "0.12.0",
    "markdown-it": "14.1.0",
    "markdown-it-block-image": "0.0.3",
    "markdown-it-footnote": "3.0.3",
    "markdown-it-texmath": "1.0.0",
    "markdown-it-toc-done-right": "4.2.0",
    "node": "^22.0.0",
    "npm-check-updates": "^17.1.3",
    "punycode": "2.1.1",
    "react": "19.0.0",
    "react-day-picker": "^9.6.2",
    "react-dom": "19.0.0",
    "react-refresh": "0.14.2",
    "rimraf": "~6.0.1",
    "serve-handler": "~6.1.5",
    "typography.css": "^0.1.0",
    "use-sync-external-store": "^1.4.0",
    "xmlhttprequest": "^1.8.0",
    "zprint-clj": "~0.8.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/postcss": "^4.0.0-alpha.42",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "chokidar": "^4.0.3",
    "chrome-remote-interface": "^0.33.2",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cljs-test": "^0.1.0",
    "karma-json-result-reporter": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "puppeteer": "^24.10.0",
    "purgecss": "^6.0.0",
    "shadow-cljs": "~2.28.18",
    "tailwindcss": "^4.0.0"
  }
}
