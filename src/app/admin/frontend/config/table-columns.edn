{:users
 {:available-columns
  [:id
   :email
   :full-name
   :status
   :role
   :last-login-at
   :email-verified-at
   :created-at
   :updated-at],
  :default-hidden-columns
  [:last-login-at :updated-at],

  :unfilterable-columns
  [:id
   :email-verified-at],

  :unsortable-columns
  [:id
   :role
   :last-login-at
   :email-verified-at
   :updated-at],
  :always-visible [:id :email],

  :computed-fields {},
  :column-config
  {:role {:width "100px", :formatter :role-badge},
   :updated-at {:width "140px", :formatter :date},
   :email {:width "200px", :formatter :email-link},
   :email-verified-at {:width "180px", :formatter :date},
   :last-login-at {:width "160px", :formatter :date},
   :status {:width "100px", :formatter :status-badge},
   :full-name {:width "180px"},
   :created-at {:width "140px", :formatter :date}}},

