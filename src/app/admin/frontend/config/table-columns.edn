 :audit-logs
 {:available-columns
  [:action
   :entity-type
   :entity-id
   :entity-name
   :admin-email
   :admin-name
   :ip-address
   :user-agent
   :changes],
  :default-hidden-columns [:entity-id :user-agent],
  :unfilterable-columns
  [:entity-id :entity-name :admin-name :changes :user-agent],
  :unsortable-columns
  [:entity-id
   :entity-name
   :admin-email
   :admin-name
   :ip-address
   :user-agent
   :changes],
  :always-visible [:action],
  :computed-fields
  {:entity-name
   {:type :join,
    :compute-fn :join-entity-name,
    :dependencies [:entity-type :entity-id]},
   :admin-email
   {:type :join,
    :compute-fn :join-admin-email,
    :dependencies [:admin-id]},
   :admin-name
   {:type :join,
    :compute-fn :join-admin-name,
    :dependencies [:admin-id]}},
  :column-config
  {:entity-type {:width "120px"},
   :user-agent {:width "200px"},
   :admin-email {:width "180px"},
   :entity-id {:width "140px"},
   :entity-name {:width "200px", :computed-field true},
   :changes {:width "220px", :formatter :collapsible-json},
   :action {:width "120px", :formatter :audit-action-badge},
   :admin-name {:width "140px"},
   :ip-address {:width "140px"}}},

